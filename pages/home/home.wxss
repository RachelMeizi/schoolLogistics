/* pages/home/home.wxss */
page{
  height: 100%;
  width: 100%;
  background: #f5f5f9;
}
.wrpg{
  width: 100%;
  /* height: 100%; */
  margin: 0 auto;
  background: #fff;
}
.userinfo {
  display: flex; 
  align-items: center;
border-bottom: 1rpx solid #f2f2f2;
}

.userinfo-avatar {
  width: 128rpx;
  height: 128rpx;
  margin: 20rpx;
  border-radius: 50%;
}

.userinfo-nickname {
  color: #000;
}

.gender{
width: 40rpx;
height: 40rpx;
}
/* 列表 */
.subjectList{
  width: 98%;
  padding: 26rpx;
  display: flex;
  border-bottom: 1rpx solid #f2f2f2;
  justify-content: space-between;
  align-items: baseline;
  font-size: 30rpx;
  
}
.subjectList image{
  width: 40rpx;
  height:40rpx;
margin-right: 10rpx;
}
