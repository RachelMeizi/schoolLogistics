page {
  overflow: hidden;
}

.container {
  color: #333;
}

.main {
  width: 100%;
  min-height: 100vh;
  position: absolute;
  left: 0;
  top: 0;
  bottom: 100rpx;
  overflow: hidden;
}

.shopping-basket {
  width: 100%;
  height: 100rpx;
  position: fixed;
  bottom: 0;
  left: 0;
}

.kaidan-height {
  padding: 72rpx 0;
}
.btn-success {
color: #fff;
background: linear-gradient(90deg, rgba(255, 184, 0, 1), rgba(255, 130, 0, 1));
border-color: #ff8200;
/* border-width: 0; */
box-shadow: 0,0,0,#fff;
border-radius:0;
width: 204rpx;

}


.has-no-goods {
  text-align: center;
  margin-top: 300rpx;
  color: #999;
}

.has-no-goods text {
  display: block;
  margin-bottom: 60rpx;
}

.has-no-goods .has-no-goods-btn {
  width: 670rpx;
  border: none;
  position: absolute;
  left: 40rpx;
  bottom: 20rpx;
}
.ball {
  width: 40rpx;
